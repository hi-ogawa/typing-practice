{
  "src": ["src/index.ts"],
  "compilerOptions": {
    "target": "ES2015",
    "lib": ["DOM", "ES6"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true
  }
}
